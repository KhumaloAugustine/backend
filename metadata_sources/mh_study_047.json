  "study_desc": {
    "title_statement": {
      "idno": "MH.INST.WHOQOL",
      "title": "WHOQOL-BREF: Quality of Life Assessment",
      "subtitle": "26-item self-report measure of quality of life"
    },
    "study_info": {
      "abstract": "The WHOQOL-BREF is a 26-item cross-cultural quality of life assessment instrument developed by the World Health Organization. It measures four domains: physical health, psychological health, social relationships, and environment. Items are rated on various scales. Suitable for clinical practice and research in diverse populations.",
      "keywords": [
        {"keyword": "quality of life"},
        {"keyword": "wellbeing"},
        {"keyword": "health-related quality of life"},
        {"keyword": "functional status"},
        {"keyword": "life satisfaction"}
      ]
    }
  },
  "instrument_details": {
    "instrument_name": "WHOQOL-BREF",
    "full_name": "World Health Organization Quality of Life Assessment - Brief Version",
    "measurement_target": "Quality of life and wellbeing",
    "num_items": 26,
    "response_scale": "Variable (1-5 point scales depending on item)",
    "score_range": "26-130",
    "subscales": ["Physical Health (7 items)", "Psychological Health (6 items)", "Social Relationships (3 items)", "Environment (8 items)"],
    "interpretation": "Higher scores indicate better quality of life",
    "psychometric_properties": {
      "reliability": "Cronbach's alpha = 0.66-0.77",
      "validity": "Good cross-cultural validity and responsiveness",
      "sensitivity": "Variable by domain"
    },
    "populations": ["General population", "Clinical samples", "Cross-cultural populations"],
    "languages": ["English", "Multiple languages available"]
  },
