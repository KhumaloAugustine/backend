  "study_desc": {
    "title_statement": {
      "idno": "MH.INST.RCADS",
      "title": "RCADS: Revised Child Anxiety and Depression Scale",
      "subtitle": "47-item scale measuring anxiety and depression in children and adolescents"
    },
    "study_info": {
      "abstract": "The RCADS is a 47-item self-report scale designed to measure anxiety and depression symptoms in children and adolescents ages 8-18. It assesses six dimensions: Separation Anxiety Disorder, Generalized Anxiety Disorder, Social Phobia, Panic Disorder/Agoraphobia, Obsessive-Compulsive Disorder, and Major Depressive Disorder. The scale is based on the DSM-IV diagnostic criteria.",
      "keywords": [
        {
          "keyword": "childhood anxiety"
        },
        {
          "keyword": "childhood depression"
        },
        {
          "keyword": "adolescent mental health"
        },
        {
          "keyword": "anxiety and depression assessment"
        }
      ]
    }
  },
  "instrument_details": {
    "instrument_name": "RCADS",
    "full_name": "Revised Child Anxiety and Depression Scale",
    "measurement_target": "Anxiety and depression in children and adolescents",
    "num_items": 47,
    "response_scale": "4-point Likert (0=Never, 1=Sometimes, 2=Often, 3=Always)",
    "score_range": "0-141",
    "subscales": [
      "Separation Anxiety Disorder (6 items)",
      "Generalized Anxiety Disorder (6 items)",
      "Social Phobia (9 items)",
      "Panic Disorder/Agoraphobia (9 items)",
      "Obsessive-Compulsive Disorder (6 items)",
      "Major Depressive Disorder (10 items)",
      "Total Anxiety Score (30 items)"
    ],
    "psychometric_properties": {
      "reliability": "Cronbach's alpha = 0.80-0.92",
      "validity": "Good convergent validity with clinical diagnoses",
      "sensitivity": "0.73-0.84",
      "specificity": "0.80-0.90"
    },
    "populations": ["Children ages 8-18", "Adolescents", "Clinical samples"],
    "languages": ["English", "Multiple languages available"]
  },
